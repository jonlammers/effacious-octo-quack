            $('#hashCpwdHiddenField').val(hashCode($('#cpwdTextBox').val()));
        });

        function hashCode(str) {
            var hash = 0;
            if (str.length < 8) return 0;
            for (i = 0; i < str.length; i++) {
                char = str.charCodeAt(i);
                hash = ((hash << 5) - hash) + char;
                hash = hash & hash; // Convert to 32bit integer
            }
            return hash;
        }
